{
  "expo": {
    "name": "Amal - أمل",
    "slug": "amal-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amal.recovery"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#09090b"
      },
      "package": "com.amal.recovery"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "apiUrl": "https://api.amal.dz"
    }
  }
}
